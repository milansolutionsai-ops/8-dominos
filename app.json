{
  "expo": {
    "name": "8 Dominos",
    "slug": "eight-dominos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/8Dominos_Logo.png",
    "scheme": "eightdominos",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/8Dominos_Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffbea"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hasslogics.eightdominos"
    },
    "android": {
      "package": "com.hasslogics.eightdominos",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/8Dominos_Logo.png",
        "backgroundColor": "#fffbea"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ae3023ed-bbe8-4cf1-951b-4206dac30888"
      }
    }
  }
}